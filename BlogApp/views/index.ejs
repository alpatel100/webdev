<% include partials/header %>

<div class="container">
    <% blogs.forEach(function(blog){ %>
    <div class="row">
      <div class="col-sm-12 col-md-12">
        <div class="thumbnail">
          <img src="<%= blog.image %>" alt="blog image">
          <div class="caption">
            <p><%= blog.created%></p>  
            <h3><%= blog.title %></h3>
            <p><%= blog.body%></p>
            <p><a href="#" class="btn btn-primary" role="button">More Info</a> <a href="#" class="btn btn-danger" role="button">Delete</a></p>
          </div>
        </div>
      </div>
    </div>
    <% }); %>
</div>
<% include partials/footer %>